#!groovy
pipeline {
    agent any
    environment { 
        CC = 'clang'
    }
    stages {
        stage('Build') {
            steps {
                sh 'printenv'
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
